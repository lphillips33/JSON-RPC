
public class Item {

	private String name;
	private double price;
	private int stock;
	
	Item(String name, double price, int stock) {
		this.name = name;
		this.price = price;
		this.stock = stock;
	}
	
	public String getName() {
		return this.name;
	}
	
	public double getPrice() {
		return this.price;
	}
	
	public int getStock() {
		return this.stock;
	}
	
	public String toString() {
		String temp = "Name: " + this.name + " " + " Price: " 
				+ price + " Stock: " + stock;
		
		return temp;
	}
}
